<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch.Vertical"
    android:background="@android:color/black">

    <include layout="@layout/toolbar" />

    <TextView
        android:id="@+id/test_scan_qrcode"
        style="@style/MatchWrapTv"
        android:text="测试扫描二维码" />

    <TextView
        android:id="@+id/test_generate_qrcode"
        style="@style/MatchWrapTv"
        android:text="测试生成和识别图片二维码" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick_gotoMap" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick_gotoLogin" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick_gotoSuccessScan" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="menu"
        android:onClick="onClick_gotoMenu" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="takephoto"
        android:onClick="onClick_gotoPhoto" />

</LinearLayout>